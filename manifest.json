{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "statapp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.03"
        },
        "dataSources" : {
            "ticketService" : {
                "uri" : "https://stats0017130098trial.hanatrial.ondemand.com/dev/dev01/statapp/services/ticket.xsodata",
                "type" : "OData",
                "settings" : {
                    "odataVersion" : "2.0",
                    "localUri" : "localService/metadata.xml",
                    "refreshAfterChange" : "true",
                    "cache" : "true"
                }
            },
            "userService" : {
                "uri" : "https://stats0017130098trial.hanatrial.ondemand.com/dev/dev01/statapp/services/user.xsodata",
                "type" : "OData",
                "settings" : {
                    "odataVersion" : "2.0",
                    "localUri" : "localService/metadata.xml"
                }
            },
            "funcAreaService" : {
                "uri" : "https://stats0017130098trial.hanatrial.ondemand.com/dev/dev01/statapp/services/functionalArea.xsodata",
                "type" : "OData",
                "settings" : {
                    "odataVersion" : "2.0",
                    "localUri" : "localService/metadata.xml"
                }
            },
            "ticketPriorityService" : {
                "uri" : "https://stats0017130098trial.hanatrial.ondemand.com/dev/dev01/statapp/services/ticketPriority.xsodata",
                "type" : "OData",
                "settings" : {
                    "odataVersion" : "2.0",
                    "localUri" : "localService/metadata.xml"
                }
            },
            "ticketStatusService" : {
                "uri" : "https://stats0017130098trial.hanatrial.ondemand.com/dev/dev01/statapp/services/ticketStatus.xsodata",
                "type" : "OData",
                "settings" : {
                    "odataVersion" : "2.0",
                    "localUri" : "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "base",
            "sap_hcb",
            "sap_ux",
            "sap_platinum",
            "sap_bluecrystal",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "statapp.view.App",
        "dependencies": {
            "minUI5Version": "1.44",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "statapp.i18n.i18n"
                }
            },
            "" : {
                "type" : "sap.ui.model.odata.v2.ODataModel",
                "dataSource" : "ticketService",
                "preload": true,
                "settings" : {
                    "setDefaultBindingMode": "TwoWay"
                }
            },
            "UserModel" : {
                "type" : "sap.ui.model.odata.v2.ODataModel",
                "settings" : {
                    "defaultOperationMode" : "Server",
                    "defaultBindingMode" : "OneTime",
                    "defaultCountMode" : "Request"
                },
                "dataSource" : "userService",
                "preload" : true
            },
            "FuncAreaModel" : {
                "type" : "sap.ui.model.odata.v2.ODataModel",
                "settings" : {
                    "defaultOperationMode" : "Server",
                    "defaultBindingMode" : "OneTime",
                    "defaultCountMode" : "Request"
                },
                "dataSource" : "funcAreaService",
                "preload" : true
            },
            "TicketPriorityModel" : {
                "type" : "sap.ui.model.odata.v2.ODataModel",
                "settings" : {
                    "defaultOperationMode" : "Server",
                    "defaultBindingMode" : "OneTime",
                    "defaultCountMode" : "Request"
                },
                "dataSource" : "ticketPriorityService",
                "preload" : true
            },
            "TicketStatusModel" : {
                "type" : "sap.ui.model.odata.v2.ODataModel",
                "settings" : {
                    "defaultOperationMode" : "Server",
                    "defaultBindingMode" : "OneTime",
                    "defaultCountMode" : "Request"
                },
                "dataSource" : "ticketStatusService",
                "preload" : true
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "statapp.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "clearTarget" : "true",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [{
                "pattern": "",
                "name": "appHome",
                "target": "home"
            }, {
				"pattern": "tickets",
				"name": "ticketList",
				"greedy": "true",
				"target": "tickets"
			}, {
				"pattern": "tickets/{ticketId}:?query:",
				"name": "ticket",
				"target": "ticketDetails"
			}, {
                "pattern": "tickets/{ticketId}/edit",
                "name": "ticketEdit",
                "target": "ticketEdit"
			}, {
                "pattern": "new",
                "name": "newTicket",
                "target": "newTicket"
			}, {
                "pattern": "settings",
                "name": "settingsHome",
                "target": "settingsHome"
			}],
            "targets": {
                "home": {
                    "viewName": "Home",
                    "viewLevel" : 1
                },
                "notFound": {
                    "viewName": "NotFound",
                    "transition": "show"
                },
				"tickets": {
					"viewPath": "statapp.view.ticket",
					"viewName": "TicketList",
					"viewLevel" : 2
				},
				"ticketDetails": {
					"viewName": "ticket.TicketDetails",
					"viewLevel" : 3
				},
				"ticketEdit": {
					"viewName": "ticket.TicketEdit",
					"viewLevel" : 4
				},
				"newTicket": {
				    "viewName": "ticket.NewTicket",
					"viewLevel" : 3
				},
				"settingsHome": {
				    "viewName": "settings.SettingsHome",
				    "viewLevel": 2
				},
				"ticketTabDetails": {
					"parent": "ticketDetails",
					"viewPath": "statapp.view.ticket.ticketDetailTabs",
					"viewName": "ticket.TicketDetailsTab",
					"viewId": "ticketTabDetailsId",
					"controlId": "detailsTab",
					"controlAggregation": "content"
				},
				"ticketTabComments": {
					"parent": "ticketDetails",
					"viewPath": "statapp.view.ticket.ticketDetailTabs",
					"viewName": "ticket.TicketCommentsTab",
					"controlId": "commentsTab",
					"controlAggregation": "content"
				},
				"ticketTabAttachements": {
					"parent": "ticketDetails",
					"viewPath": "statapp.view.ticket.ticketDetailTabs",
					"viewName": "ticket.TicketAttachmentsTab",
					"controlId": "attachmentsTab",
					"controlAggregation": "content"
				}
            }
        }
    }
}
