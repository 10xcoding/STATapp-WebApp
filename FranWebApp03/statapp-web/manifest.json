{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "sap.ui.demo.nav",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.03"
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "sap.ui.demo.nav.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.ui.demo.nav.i18n.i18n"
                }
            },
            "ticket": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "https://stats0017130098trial.hanatrial.ondemand.com/dev/dev01/statapp/services/ticket.xsodata/Tickets/?$format=json"
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "sap.ui.demo.nav.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [{
                "pattern": "",
                "name": "appHome",
                "target": "home"
            }, {
				"pattern": "tickets",
				"name": "ticketList",
				"target": "tickets"
			}, {
				"pattern": "tickets/{ticketId}",
				"name": "ticketDetail",
				"target": "ticketDetail"
			}],
            "targets": {
                "home": {
                    "viewName": "Home",
                    "viewLevel" : 1
                },
                "notFound": {
                    "viewName": "NotFound",
                    "transition": "show"
                },
				"tickets": {
					"viewPath": "sap.ui.demo.nav.view.ticket",
					"viewName": "TicketList",
					"viewLevel" : 2
				},
				"ticketDetail": {
					"viewName": "ticket.TicketDetail",
					"viewLevel" : 3
				}
            }
        }
    }
}